<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="150.0" prefWidth="300.0" style="-fx-background-color: #6495ED;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox alignment="CENTER" layoutX="50.0" layoutY="25.0" prefHeight="100.0" prefWidth="200.0" spacing="10.0" style="-fx-background-color: #ffffff; -fx-padding: 10px;">
         <children>
            <Label fx:id="messageLabel" style="-fx-font-size: 14pt; -fx-text-fill: #000000;" text="Congratulations!" wrapText="true" />
            <Button fx:id="okButton" alignment="CENTER" mnemonicParsing="false" style="-fx-font-size: 12pt; -fx-background-color: #6495ED; -fx-text-fill: #ffffff;" text="OK" />
         </children>
      </VBox>
   </children>
</AnchorPane>
