<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
minHeight="1000.0" minWidth="1920.0" prefHeight="1000.0"
	prefWidth="1920.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="Controller.EditPageController">
	<children>
		<ImageView fitHeight="1080" fitWidth="1920" layoutX="-2.0"
			opacity="0.87" pickOnBounds="true">
			<image>
				<Image url="@/Image/qEditBG.jpg" />
			</image>
		</ImageView>

		<Text fill="RED" layoutX="74.0" layoutY="56.0"
			strokeType="OUTSIDE" strokeWidth="0.0" text="Question to Edit:"
			AnchorPane.leftAnchor="400.0" AnchorPane.topAnchor="200">
			<font>
				<Font size="18.0" />
			</font>
		</Text>
		<Button layoutX="152.0" layoutY="227.0" mnemonicParsing="false"
			text="Save" onAction="#saveEditedQuestion" AnchorPane.leftAnchor="720.0" AnchorPane.topAnchor="400"/>
		<Button layoutX="216.0" layoutY="227.0" mnemonicParsing="false"
			text="Cancel" onAction="#cancelButtonClicked" AnchorPane.leftAnchor="800.0" AnchorPane.topAnchor="400"/>
		<ChoiceBox fx:id="questionChoiceBox" layoutX="77.0"
			layoutY="69.0" prefHeight="26.0" prefWidth="431.0" AnchorPane.leftAnchor="550.0" AnchorPane.topAnchor="200"/>
		<Text fill="RED" layoutX="73.0" layoutY="135.0"
			strokeType="OUTSIDE" strokeWidth="0.0" text="Enter a question:" AnchorPane.leftAnchor="400.0" AnchorPane.topAnchor="280">
			<font>
				<Font size="18.0" />
			</font>
		</Text>
		<TextField fx:id="editedQuestionTextField" layoutX="78.0"
			layoutY="152.0" prefHeight="26.0" prefWidth="431.0" AnchorPane.leftAnchor="550.0" AnchorPane.topAnchor="280"/>
	</children>
</AnchorPane>
